<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/generate_pdf"
        android:layout_width="152dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="@string/generate_pdf"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.536"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/generate_pdf_2"
        android:layout_width="152dp"
        android:layout_height="wrap_content"
        android:text="@string/generate_pdf_2"
        app:layout_constraintEnd_toEndOf="@+id/generate_pdf"
        app:layout_constraintStart_toStartOf="@+id/generate_pdf"
        app:layout_constraintTop_toBottomOf="@+id/generate_pdf" />


</androidx.constraintlayout.widget.ConstraintLayout>